{
  "buildOptions": {
    "lib": ["es2021"],
    "module": "commonjs",
    "target": "es2021",

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strict": false
  }
}
